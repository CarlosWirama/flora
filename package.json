    "react-materialize": "^2.6.0",
    "react-router-dom": "^4.3.1",
    "react-router-tabs": "^1.2.0",
    "react-transition-group": "^2.5.1",
    "styled-components": "^4.1.3",
    "webpack": "^3.12.0",
