{
  "name": "flora",
  "version": "0.0.1",
  "description": "A web catalog for my personal project; Using ReactJS with MobX, NodeJS with Express JS, and MongoDB with Mongoose",
  "scripts": {
    "start": "webpack-dev-server --hot --open",
    "lint": "eslint webclient/src",
    "client": "webpack-dev-server --hot --open",
    "server": "nodemon server/server",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "proxy": "http://localhost:3001/",
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlosWirama/flora.git"
  },
  "author": "CarlosWirama <vincentius_carlos@yahoo.co.id> (http://github.com/CarlosWirama)",
  "license": "MIT",
  "homepage": "http://github.com/CarlosWirama",
  "devDependencies": {
    "mobx": "^3.0.0",
    "mobx-react": "^4.4.3",
    "mobx-react-devtools": "^4.2.11",
    "nodemon": "^1.17.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.5.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.9",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "main": "webpack.config.js",
  "bugs": {
    "url": "https://github.com/CarlosWirama/flora/issues"
  }
}
